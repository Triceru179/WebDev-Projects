<%- include('partials/header'); %>
<h1>Compose</h1>
<form action="/compose" method="POST">
        <div class="form-group">
        <label for="publicationTitle">Title</label>
        <input class="form-control" type="text" name="publicationTitle" autocomplete="off">
        <label for="publicationContent">Post</label>
        <textarea class="form-control" name="publicationContent" rows="5" cols="30"></textarea>
    </div>
    <button class="btn btn-primary button" type="submit" name="button">Publish</button>
</form>
<%- include('partials/footer'); %>